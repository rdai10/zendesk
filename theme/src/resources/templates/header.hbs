								url: 'https://customer.liferay.com/activation-key'
							},
							{
								description: '{{dc 'mega_menu-knowledge_base_description'}}',
								name: '{{dc 'mega_menu-knowledge_base'}}',
								svgId: '#knowledge-base',
								url: '/hc/categories/360000580671'
							},
