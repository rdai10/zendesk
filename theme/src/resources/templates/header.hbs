<header class="header" id="header">
	<div class="container-fluid container-fluid-max-xl">
		<div class="header-inner row">
			<div class="col-3 header-content">
				{{#link 'help_center' class="header-link"}}
					<svg alt="{{help_center.name}}" class="header-logo">
						<use xlink:href="#liferay-waffle" />
					</svg>
				{{/link}}

				{{#link 'help_center'}}
					<h4 class="site-name">
						{{dc 'help_center'}}
					</h4>
				{{/link}}
			</div>

			<div class="col-5 header-content header-menu-list secondary-text-color">
				<div class="list-item">
					<h5>
						{{dc 'products'}}
					</h5>

					<div class="header-menu" id="products"></div>
				</div>

				<div class="list-item">
					<h5>
						{{dc 'resources'}}
					</h5>

					<div class="header-menu" id="resources"></div>
				</div>

				<div class="list-item">
					<h5>
						{{dc 'security'}}
					</h5>

					<div class="header-menu" id="security"></div>
				</div>

				<div class="list-item">
					<h5>
						{{dc 'support'}}
					</h5>

					<div class="header-menu" id="support"></div>
				</div>
			</div>

			<div class="col-4 header-content user-wrapper">
				{{search class="search-nav" submit=false}}

				<div class="user">
					{{#if signed_in}}
						{{user_info}}
					{{else}}
						{{#link 'sign_in' class="btn btn-primary"}}
							Sign In
						{{/link}}
					{{/if}}
				</div>
			</div>
		</div>
	</div>
</header>

<script>
	$(function() {
		const accountLinks = {
			className: 'account-links',
			configs: [
				{
					name: '{{t 'my_requests'}}',
					url: '{{page_path 'requests'}}'
				},
				{
					name: '{{dc 'project_details'}}',
					url: ''
				}
			]
		};

		const highlightedLinksConfigs = [
			{
				name: '{{dc 'contact_us'}}',
				svgId: '#contact',
				url: ''
			}
		];

		const userTags = window.HelpCenter.user.tags;

		if (Liferay.hasCreateTicketPermission(userTags)) {
			highlightedLinksConfigs.unshift(
				{
					name: '{{dc 'submit_a_ticket'}}',
					svgId: '#ticket',
					url: '{{page_path 'new_request'}}'
				}
			);
		}

		const highlightedLinks = {
			className: 'highlighted',
			configs: highlightedLinksConfigs
		};

		const locale = {
			currentLocale: {
				name: {{json_stringify current_locale.name}},
				url: {{json_stringify current_locale.url}}
			},
			alternativeLocales: [
				{{#each alternative_locales}}
					{
						name: {{json_stringify name}},
						url: {{json_stringify url}}
					},
				{{/each}}
			]
		};

		Liferay.render(
			Liferay.MegaMenu,
			{
				accountLinks: accountLinks,
				cardMenuItems: {
					className: 'card-menu',
					configs: [
						{
							name: 'DXP 7.1',
							svgId: '#dxp-logo',
							url: ''
						},
						{
							name: 'DXP 7.0',
							svgId: '#7-0-logo',
							url: ''
						},
						{
							name: '{{dc 'commerce'}}',
							svgId: '#commerce-logo',
							url: ''
						},
						{
							name: '{{dc 'analytics_cloud'}}',
							svgId: '#analytics-cloud-logo',
							url: 'https://analytics.liferay.com'
						},
						{
