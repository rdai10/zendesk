						className: 'card-menu',
						configs: [
							{
								description: '{{dc 'mega_menu-overview_description'}}',
								name: '{{dc 'mega_menu-overview'}}',
								svgId: '#security-overview',
								url: '/hc/articles/360018999331'
							},
							{
								description: '{{dc 'mega_menu-reporting_security_issues_description'}}',
								name: '{{dc 'mega_menu-reporting_security_issues'}}',
								svgId: '#report-security-issues',
								url: '/hc/articles/360018999331#Reporting-Security-Issues'
							},
							{
								description: '{{dc 'mega_menu-security_advisories_description'}}',
								name: '{{dc 'mega_menu-security_advisories'}}',
								svgId: '#security-advisories',
								url: '/hc/articles/360018999691'
							},
							{
								description: '{{dc 'mega_menu-security_and_disclosure_policy_description'}}',
								name: '{{dc 'mega_menu-security_and_disclosure_policy'}}',
								svgId: '#security-policy',
								url: '/hc/articles/360018999331#Liferay-Security-Policy'
							}
						]
					},
					highlightedLinks: highlightedLinks,
					locale: locale,
					name: '{{dc 'security'}}'
				},
				document.getElementById('megaMenuSecurity')
			);

			Liferay.render(
				Liferay.MegaMenu,
				{
					accountLinks: accountLinks,
					cardMenuItems: {
						className: 'card-menu',
						configs: [
							{
								description: '{{dc 'mega_menu-compatibility_matrix_description'}}',
								name: '{{dc 'mega_menu-compatibility_matrix'}}',
								svgId: '#compatibility-matrix',
								url: '/hc/sections/360002100591'
							},
							{
								description: '{{dc 'mega_menu-service_life_description'}}',
								name: '{{dc 'mega_menu-service_life'}}',
								svgId: '#end-of-life',
								url: '/hc/sections/360002103332'
							},
							{
								description: '{{dc 'mega_menu-fix_delivery_method_description'}}',
								name: '{{dc 'mega_menu-fix_delivery_method'}}',
								svgId: '#fix-delivery-method',
								url: '/hc/articles/360018693032'
							},
							{
								description: "{{dc 'mega_menu-support_coverage_description'}}",
								name: '{{dc 'mega_menu-support_coverage'}}',
								svgId: '#support-coverage',
								url: '/hc/articles/360018692772'
							},
							{
								description: '{{dc 'mega_menu-support_faqs_description'}}',
								name: '{{dc 'mega_menu-support_faqs'}}',
								svgId: '#support-faqs',
								url: '/hc/articles/360018999191'
							},
							{
								description: '{{dc 'mega_menu-service_levels_description'}}',
								name: '{{dc 'mega_menu-service_levels'}}',
								svgId: '#support-overview',
								url: '/hc/articles/360018999271'
							}
						]
					},
